{
  "$schema": "http://json-schema.org/schema",
  "id": "SchematicsVaDataServiceMethod",
  "title": "Add method with http call to data service.",
  "type": "object",
  "properties": {
    "dataServiceFilePath": {
      "type": "string",
      "description": "Data service's path.",
      "x-prompt": "What is the data service's path?"
    },
    "name": {
      "type": "string",
      "description": "Mehtod's name.",
      "x-prompt": "What is the method's name?"
    },
    "httpMethod": {
      "type": "string",
      "description": "Http method to use.",
      "enum": ["GET", "POST", "PUT", "PATCH", "DELETE"],
      "x-prompt": "What http method should be used?"
    },
    "properties": {
      "type": "string",
      "description": "Method's properties.",
      "x-prompt": "Provide methods properties: ",
      "default": ""
    },
    "returnType": {
      "type": "string",
      "description": "Method's return type.",
      "x-prompt": "What is the return type?",
      "default": "any"
    },
    "responseType": {
      "type": "string",
      "description": "Response return type.",
      "x-prompt": "What is the response return type?",
      "default": "any"
    },
    "mapResponse": {
      "type": "string",
      "description": "Add map operator to response pipe.",
      "x-prompt": "What is response object path to map?"
    }
  }
}
