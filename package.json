{
  "name": "va-schematics",
  "version": "0.0.0",
  "description": "ValueAdd schematics",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "lint": "./node_modules/.bin/tslint --project tsconfig.json ./src/**/*.ts --exclude **/*.d.ts",
    "test": "npm run build && node jasmine.js",
    "test:cov": "istanbul cover --include-all-sources --root ./src jasmine.js && istanbul check-coverage",
    "watch": "./node_modules/.bin/nodemon --watch src --ext ts,json --ignore *.d.ts --exec 'npm run test'",
    "validate": "node ./tools/tasks/validate-commit-message.js"
  },
  "keywords": [
    "schematics"
  ],
  "author": "",
  "license": "MIT",
  "schematics": "./src/collection.json",
  "dependencies": {
    "@angular-devkit/core": "^7.3.8",
    "@angular-devkit/schematics": "^7.3.8",
    "@schematics/angular": "^7.3.8",
    "fs-extra": "^7.0.1",
    "import": "0.0.6"
  },
  "devDependencies": {
    "@types/fs-extra": "^5.0.5",
    "@types/jasmine": "^3.3.12",
    "@types/node": "^11.13.7",
    "coveralls": "^3.0.3",
    "husky": "^1.3.1",
    "istanbul": "^0.4.5",
    "jasmine": "^3.4.0",
    "jasmine-console-reporter": "^3.1.0",
    "jasmine-node": "^1.16.2",
    "jasmine-supertest": "^1.0.0",
    "lint-staged": "^8.1.5",
    "nodemon": "^1.18.11",
    "prettier": "^1.17.0",
    "semver": "^6.0.0",
    "shelljs": "^0.8.3",
    "standard-version": "^5.0.2",
    "supertest": "^4.0.2",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-sonarts": "^1.9.0",
    "typescript": "~3.4.5"
  }
}
